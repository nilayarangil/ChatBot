@{
}
@model ChatBot.Models.Entities.ChatbotQA


<h1>Add Question and Answers</h1>

<form method="post">
    <div class="mt-3">
        <label class="form-label">Question</label>
        <input type="text" class="form-control" asp-for="Question" />
    </div>
    <div class="mt-3">
        <label class="form-label">Answer</label>
        <input type="text" class="form-control" asp-for="Answer" />
    </div>    
    <div class="mt-3">
        <button type="submit" class="btn btn-success">Submit</button>
    </div>
</form>